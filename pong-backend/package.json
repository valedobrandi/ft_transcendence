{
    "name": "pong-backend",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "prestart": "npm run build",
        "start": "node --trace-uncaught --trace-warnings dist/start.js",
        "test": "vitest --run --reporter verbose",
        "database": "npx tsx /app/src/database/init.ts",
        "print": "npx tsx /app/src/database/table_print.ts",
        "production:print": "node /app/dist/database/table_print.js",
        "dev": "nodemon",
        "test:ai": "ts-node src/classes/test-ai.ts"
    },
    "dependencies": {
        "@fastify/cookie": "^11.0.2",
        "@fastify/cors": "^11.1.0",
        "@fastify/jwt": "^10.0.0",
        "@fastify/multipart": "^9.3.0",
        "@fastify/static": "^8.3.0",
        "@fastify/websocket": "^11.2.0",
        "bcrypt": "^6.0.0",
        "better-sqlite3": "^12.4.1",
        "ethers": "^6.15.0",
        "fastify": "^5.6.1",
        "jsonwebtoken": "^9.0.2",
        "node-fetch": "^3.3.2",
        "pino-pretty": "^13.1.2",
        "qrcode": "^1.5.4",
        "resend": "^6.3.0",
        "speakeasy": "^2.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/node": "^22.18.11",
        "@types/qrcode": "^1.5.6",
        "@types/speakeasy": "^2.0.10",
        "@types/ws": "^8.18.1",
        "ethers": "^6.15.0",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.6",
        "typescript": "~5.8.0",
        "vitest": "^4.0.4",
        "ws": "^8.13.0"
    }
}
